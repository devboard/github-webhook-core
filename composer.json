{
    "name": "devboard/github-webhook-core",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Devboard\\GitHub\\Webhook\\Core\\": "src/",
            "tests\\Devboard\\GitHub\\Webhook\\Core\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Devboard\\GitHub\\Webhook\\Core\\": "spec/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "devboard/github-lib": "^0.6.0",
        "webmozart/assert": "^1.2"
    },
    "require-dev": {
        "ciaranmcnulty/phpspec-typehintedmethods": "^2.0",
        "codeclimate/php-test-reporter": "^0.4.4",
        "devboard/thesting-data-source": "^0.5.0",
        "friendsofphp/php-cs-fixer": "^2.3",
        "henrikbjorn/phpspec-code-coverage": "^3.0",
        "humbug/humbug": "~1.0@dev",
        "mockery/mockery": "^0.9.9",
        "nikic/php-parser": "^3.0",
        "phing/phing": "^2.16",
        "phpmd/phpmd": "^2.6",
        "phpspec/phpspec": "^3.4",
        "phpspec/prophecy": "^1.7",
        "phpstan/phpstan": "^0.7.0",
        "phpunit/phpunit": "^5.7.21",
        "satooshi/php-coveralls": "^1.0",
        "sebastian/phpcpd": "^3.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.1.0"
        },
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    }
}
