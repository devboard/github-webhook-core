{
    "name": "devboard/github-webhook-core",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Devboard\\GitHub\\Webhook\\Core\\": "src/",
            "tests\\Devboard\\GitHub\\Webhook\\Core\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Devboard\\GitHub\\Webhook\\Core\\": "spec/"
        }
    },
    "require": {
        "php":                                      ">=7.1.0",
        "devboard/github-lib":                      "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit":                          "^5",
        "mockery/mockery":                          "~0.9",
        "phpspec/phpspec":                          "~3.0",
        "henrikbjorn/phpspec-code-coverage":        "^3.0",
        "ciaranmcnulty/phpspec-typehintedmethods":  "~2.0",
        "phpmd/phpmd":                              "~2.1",
        "squizlabs/php_codesniffer":                "2.*",
        "friendsofphp/php-cs-fixer":                "^2.0",
        "satooshi/php-coveralls":                   "^1",
        "codeclimate/php-test-reporter":            "^0.4.2",
        "sebastian/phpcpd":                         "^3.0",
        "phing/phing":                              "^2.15",
        "phpstan/phpstan":                          "^0.6.1",
        "humbug/humbug":                            "~1.0@dev"
    },
    "config": {
        "platform": {
            "php": "7.1.0"
        },
        "bin-dir": "bin"
    }
}
